                <div class="tab-content">
                    <div class="tab-pane fade show active" id="items">
                        <h3>Items Tab Content</h3>
                        <!-- Add your Items tab content here -->
                    </div>
                    <div class="tab-pane fade" id="itemsList">
                        <h3>Items List Tab Content</h3>
                        <!-- Add your Items List tab content here -->
                    </div>
                    <div class="tab-pane fade" id="customers">
